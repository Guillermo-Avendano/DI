<RocService stylesheet="com/rochade/wa/api/resource/stylesheets/abspathV2.xsl, com/rochade/wa/api/resource/stylesheets/rsV2.xsl, com.rochade.wa.api.ML.java" 
 whatdefault="id, name, DEFINITION" what="{$what}">
 <!-- The parameter what is used to define what information is of interest. Each keyword specified in what represents a column in the output ResultSet. -->
  
   <connection
         physicalDefinitions    ="startup"
         applicationDefinitions = "applications_rir"
         id="c1"/>
  <categories>
    
   
   <category id="InsertMetaNote" name="InsertMetaNote" >
        <insert_into itemtype="EIM_META_NOTE" 
                      scopeID="0" scopePath="" >
          <value attribute="name" details="{$name}"/>
          <value attribute="EIM_MESSAGE" details="{$message}" />
          <value attribute="DEFINITION" details="{$definition}"/>   
          <value attribute="EIM_ANNOTATED_ITEM" details="{$term}"/>          
        </insert_into> 
        
     </category>
      
  </categories>
  <request>
    <!-- <maxHits>${maxHits}</maxHits> -->
    <task mand="true">${task}</task>
    <category>${category}</category>
    <searchString>${searchString}</searchString>
  </request>
</RocService>