//votesToComplete=roundUp(percentageToComplete.toLong()/100*cnt)
//votesToReject=roundUp(percentageOfVotesToReject.toLong()/100*votesToComplete)
//votesToReassign=roundUp(percentageOfVotesToReassign.toLong()/100*votesToComplete)
