<?xml-stylesheet type="text/xsl" href="Resources.xsl"?>
<ShapeResources
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ShapeRes.xsd">
	<TableColumn Title="%3rdPartyemail" ID="3rdPartyemail"
		Width="1">
		<ColumnText Key="DWR_ORG_EMAIL" />
	</TableColumn>
	<TableColumn Title="%3rdPartyLocation" ID="3rdPartyLocation" Width="3">
		<ColumnText Key="DWR_ORG_LOCATION" />
	</TableColumn>
	<TableColumn Title="%dynEntity" ID="3rdorgunit" Width="3">
	   <ColumnList Direction="forward" ObjectType="DWR_ORG_ORGANIZATIONAL_UNIT" AssocType="DWR_ORG_BELONGS_TO_ORG_UNIT">
			<ColumnText Key="name" />
		</ColumnList>	
	</TableColumn>
	
	<Table ID="GDPR_THIRD_PARTY">
		<TableColumn ref="dynMgActions" Filterable="false" />
		<TableColumn ref="dynMLNameWithIcon" Sort="0"
			Object.Use="assoc.dst" />
		<!-- <TableColumn ref="dynMgDefinition" Object.Use="assoc.dst" /> -->
		<TableColumn ref="3rdPartyLocation" Sort="0"
			Object.Use="assoc.dst" />
		<TableColumn ref="3rdPartyemail" Sort="0"
			Object.Use="assoc.dst" />
		<TableColumn ref="3rdorgunit" Sort="0" Object.Use="assoc.dst" />
		<Property Name="pageSize" Value="5" />
		<Property Name="lineHeight" Value="1" />
	</Table>

	<Table ID="GDPR_PII_STRUCTURE">
		<TableColumn ref="dynMgActions" Filterable="false" />
		<TableColumn ref="dynMLNameWithIcon" Sort="0"
			Object.Use="assoc.dst" />
		<TableColumn ref="dynMgDefinition" Object.Use="assoc.dst" />
		<Property Name="pageSize" Value="5" />
		<Property Name="lineHeight" Value="1" />
	</Table>
	<Table ID="GDPR_BAPPLICATION">
		<TableColumn ref="dynMgActions" Filterable="false" />
		<TableColumn ref="dynMLNameWithIcon" Sort="0"
			Object.Use="assoc.dst" />
		<TableColumn ref="dynMgDefinition" Object.Use="assoc.dst" />
		<Property Name="pageSize" Value="5" />
		<Property Name="lineHeight" Value="1" />
	</Table>
	<Table ID="GDPR_BINFORMATION">
		<TableColumn ref="dynMgActions" Filterable="false" />
		<TableColumn ref="dynMLNameWithIcon" Sort="0"
			Object.Use="assoc.dst" />
		<TableColumn ref="dynMgDefinition" Object.Use="assoc.dst" />
		<Property Name="pageSize" Value="5" />
		<Property Name="lineHeight" Value="1" />
	</Table>
</ShapeResources>