			<value label="$Message.CategoryQuery.value.time.past24hour" name="PAST-24-HOUR">
				<param label="$Message.CategoryQuery.param.date1" name="Date1" rule="{$NOW}-24*{$HOUR}" />
				<param label="$Message.CategoryQuery.param.date2" name="Date2" rule="{$NOW}" />
