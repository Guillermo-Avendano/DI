<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<RocService stylesheet="com\rochade\wa\api\resource\stylesheets\rs.xsl" whatdefault="id, type, name" what="{$what}"
	name="HomeSearchCategoryQuery">
	<!-- $CRETIME, $UPDTIME scopeId, -->
	<categories>
		<patterns>
			<!-- <pattern id="1" task="Search For Types with Lucene"> <bindPatternGroup idref="pg2"/> <LUCENE>{$SearchString} AND type:{$Type}</LUCENE> 
				</pattern> -->
			<pattern id="p1">
				<SDCAC xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://schema.asg.com/rochade/query/Sdcac-1.0.xsd">
					<CONDITION>
						<AND>
							<OR>
								<rule forall="//category[@id='{$actCategory}']/searchName" optional="propRefs">
									<param label="$Message.CategoryQuery.param.str" name="str" rule="com.rochade.wa.api.rule.UnfoldBooleanExpr" value="@param" />
									<XNAME text="(XNAME * * ~/ @*{$str}@*)" />
								</rule>
								<rule forall="//category[@id='{$actCategory}']/searchAttr" optional="propRefs">
									<param label="$Message.CategoryQuery.param.attrname" name="attrname" value="@param1" />
									<param label="$Message.CategoryQuery.param.str" name="str" rule="com.rochade.wa.api.rule.UnfoldBooleanExpr" value="@param2" />
									<TA text="(TA {$attrname} ~/ @*{$str}@*)" />
								</rule>
								<rule forall="//category[@id='{$actCategory}']/searchNameAttr" optional="propRefs">
									<param label="$Message.CategoryQuery.param.attrname" name="attrname" value="@param1" />
									<param label="$Message.CategoryQuery.param.str" name="str" rule="com.rochade.wa.api.rule.UnfoldBooleanExpr" value="@param2" />
									<TA text="(TA {$attrname} ~/ @*{$str}@*)" />
								</rule>
								<rule forall="//category[@id='{$actCategory}']/searchInNameAttrOption">
									<param label="$Message.CategoryQuery.param.value" name="valuemore" wildcard="false"
										rule="com.rochade.wa.api.rule.UnfoldBooleanExpr" value="@searchInNameAttrSelMore" />
									<param label="$Message.CategoryQuery.param.str" name="str" rule="com.rochade.wa.api.rule.UnfoldBooleanExpr" value="@searchInNameAttrString" />
									<TA text="(TA {$valuemore} ~/ @*{$str}@*) " />
								</rule>
							</OR>
							<rule forall="//category[@id='{$actCategory}']/type">
								<param label="$Message.CategoryQuery.param.type" name="type" value="@param" />
								<TYPE text="(TYPE = {$type})" />
							</rule>
							<rule forall="//category[@id='{$actCategory}']/super">
								<param label="$Message.CategoryQuery.param.type" name="type" value="@param" />
								<SUPER text="(SUPER = {$type})" />
							</rule>
							<rule forall="//category[@id='{$actCategory}']/taEquals">
								<param label="$Message.CategoryQuery.param.attr" name="attr" value="@param1" />
								<param label="$Message.CategoryQuery.param.val" name="val" value="@param2" />
								<TA text="(TA {$attr} = STR {$val})" />
							</rule>
							<rule forall="//category[@id='{$actCategory}']/taEqualsOrEmpty">
								<param label="$Message.CategoryQuery.param.attr" name="attr" value="@param1" />
								<param label="$Message.CategoryQuery.param.val" name="val" value="@param2" />
								<TA text="( (TA {$attr} = STR {$val}) OR (TA_SIZE {$attr} = 0) )" />
							</rule>
							<rule forall="//category[@id='{$actCategory}']/relToBT">
								<param label="$Message.CategoryQuery.param.term" name="term" value="@param" />
								<LA text="(LA RBG/REFERENCES &lt;- SUPER RBG/BUSINESS-TERM NAME = {$term})" />
							</rule>
							<rule forall="//category[@id='{$actCategory}']/relToBTID">
								<param label="$Message.CategoryQuery.param.termid" name="termID" value="@param" />
								<LA text="(LA RBG/REFERENCES &lt;- ITEMID {$termID})" />
							</rule>
							<rule forall="//category[@id='{$actCategory}']/taEqualsNot">
								<param label="$Message.CategoryQuery.param.attr" name="attr" value="@param1" />
								<param label="$Message.CategoryQuery.param.val" name="val" value="@param2" />
								<TA text="NOT (TA {$attr} = STR {$val})" />
							</rule>

							<rule forall="//category[@id='{$actCategory}']/timeOption">
								<param label="$Message.CategoryQuery.param.c_attr" name="c_attr" value="//option[@id='{$optionId}']/@attr">
									<param label="$Message.CategoryQuery.param.optionid" name="optionId" value="@optionRef" />
								</param>
								<param label="$Message.CategoryQuery.param.date1" name="date1" rule="com.rochade.wa.api.rule.DateRule"
									value="//valDom[@id='{$vd}']/value[@name='{$value}']/param[@name='Date1']/@rule">
									<param label="$Message.CategoryQuery.param.vd" name="vd" value="//option[@id='{$optionid}']/@valDomRef">
										<param label="$Message.CategoryQuery.param.optionid" name="optionid" value="@optionRef" />
									</param>
									<param label="$Message.CategoryQuery.param.value" name="value" value="@timeOptionKind" />
								</param>
								<param label="$Message.CategoryQuery.param.date2" name="date2" rule="com.rochade.wa.api.rule.DateRule"
									value="//valDom[@id='{$vd}']/value[@name='{$value}']/param[@name='Date2']/@rule">
									<param label="$Message.CategoryQuery.param.vd" name="vd" value="//option[@id='{$optionid}']/@valDomRef">
										<param label="$Message.CategoryQuery.param.optionid" name="optionid" value="@optionRef" />
									</param>
									<param label="$Message.CategoryQuery.param.value" name="value" value="@timeOptionKind" />
								</param>
								<SA text="(SA {$c_attr} &gt;= DATE {$date1}) AND (SA {$c_attr} &lt;= DATE {$date2})" />
							</rule>

							<rule forall="//category[@id='{$actCategory}']/stageOption">
								<param label="$Message.CategoryQuery.param.attr" name="attr" value="//option[@id='{$optionId}']/@attr">
									<param label="$Message.CategoryQuery.param.optionid" name="optionId" value="@optionRef" />
								</param>
								<param label="$Message.CategoryQuery.param.val1" name="val1" value="//valDom[@id='{$vd}']/value[@name='{$value}']/@val">
									<param label="$Message.CategoryQuery.param.vd" name="vd" value="//option[@id='{$optionid}']/@valDomRef">
										<param label="$Message.CategoryQuery.param.optionid" name="optionid" value="@optionRef" />
									</param>
									<param label="$Message.CategoryQuery.param.value" name="value" value="@stageName" />
								</param>
								<TA text="(TA {$attr} = STR {$val1})" />
							</rule>
						</AND>
					</CONDITION>
				</SDCAC>
			</pattern>
		</patterns>
		<option attr="$CRETIME" id="cretimeOption" label="$Message.CategoryQuery.option.created" name="created" valDomRef="vd1" />

		<option attr="$UPDTIME" id="updtimeOption" label="$Message.CategoryQuery.option.modified" name="modified" valDomRef="vd1" />

		<valDom id="vd1" label="$Message.CategoryQuery.valdom.intervals" name="Intervals">
