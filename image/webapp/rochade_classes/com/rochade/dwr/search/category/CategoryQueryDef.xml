				<param label="$Message.CategoryQuery.param.date2" name="Date2" rule="{$NOW}" />
			</value>
			<value label="$Message.CategoryQuery.value.time.pastyear" name="PAST-YEAR">
				<param label="$Message.CategoryQuery.param.date1" name="Date1" rule="{$NOW}-1*{$YEAR}" />
				<param label="$Message.CategoryQuery.param.date2" name="Date2" rule="{$NOW}" />
			</value>
			<value label="$Message.CategoryQuery.value.time.range" name="RANGE">
				<param label="$Message.CategoryQuery.param.date1" name="Date1" rule="{$paramBegin}" />
				<param label="$Message.CategoryQuery.param.date2" name="Date2" rule="{$paramEnd}" />
			</value>
		</valDom>

