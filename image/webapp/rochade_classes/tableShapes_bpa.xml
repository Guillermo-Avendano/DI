			Renderer="com.rochade.bpa.cellrenderer.BPContextPathCellRenderer" />
	</TableColumn>

	<TableColumn Title="" ID="dynBpaNameNoHeader">
		<ColumnText Key="name" />
	</TableColumn>

	<TableColumn Title="%dynBpaName" ID="dynBpaName">
		<ColumnText Key="name" />
	</TableColumn>

	<TableColumn Title="%dynBpaName" ID="dynBpaMLNameWithIcon" Width="1" >
		<ColumnText Key="languages" />
		<ColumnText Key="name" />
		<ColumnProperty Renderer="com.rochade.wa.view.table.WAMLNameWithIconCellRenderer" />
	</TableColumn>

	<TableColumn Title="%dynDef" ID="dynBpaMLDefinition" Width="3">
		<ColumnText Key="DEFINITION" />
	</TableColumn>

	<TableColumn Title="%dynGoverningContext" ID="dynBPOwnedBy" Width="1">
		<ColumnText Key="name" />
		<ColumnProperty Renderer="com.rochade.bpa.cellrenderer.BPContextCellRenderer" />
	</TableColumn>

	<TableColumn Title="%dynCrDate" ID="dynBpaCrDate">
		<ColumnText Key="crDate" />
		<ColumnProperty Renderer="com.rochade.mg.cellrenderer.DateCellRenderer" />
	</TableColumn>

	<TableColumn Title="%dynModifDate" ID="dynBpaModifDate">
		<ColumnText Key="upDate" />
		<ColumnProperty Renderer="com.rochade.mg.cellrenderer.DateCellRenderer" />
	</TableColumn>

	<TableColumn Title="%dynStatus" ID="dynBpaStatus" Width="1">
		<ColumnText Key="RBG/STATUS" />
		<ColumnProperty Renderer="com.rochade.mg.cellrenderer.BtStatusCellRenderer" />
	</TableColumn>

	<Table ID="BI_Items">
		<TableColumn ref="dynActions" Filterable ="false"/>
		<TableColumn ref="dynBpaName" Sort="0" Object.Use="assoc.dst" />
		<TableColumn ref="dynQualifiedNameWidth2" Visible="false" Object.Use="assoc.dst" />
		<TableColumn ref="dynType" Object.Use="assoc.dst" />
		<TableColumn ref="dynDefWidth2" Object.Use="assoc.dst" />
	</Table>

	<Table ID="BI_Items_with_context">
		<TableColumn ref="dynBpaName" Sort="0" />
		<TableColumn ref="dynQualifiedNameWidth2" Visible="false" Object.Use="assoc.dst" />
		<TableColumn ref="dynType" />
		<TableColumn ref="dynDefWidth2" />
		<TableColumn ref="dynBpaBusinessContext" />
		<TableColumn ref="dynTags" Visible="false" />
		<Property Name="baseType" Value="BPK__BUSINESS_ITEM" />
		<Property Name="ignoreAttributesOfTypes" Value="RBG/BUSINESS-TERM, BPK_BUSINESS_RULE, BPK_PROCESS, BPK_CRITICAL_DATA_ELEMENT, GDP_PERSONAL_INFORMATION, GDP_PI_STRUCTURE, BPK_POLICY, BPK_STANDARD, BPK__CUST_TIMED_BUSINESS_ITEM, BPK__CUST_BUSINESS_ITEM" />
	</Table>

	<Table ID="BI_Items_with_def">
		<TableColumn ref="dynBpaName" Sort="0" Object.Use="assoc.dst"/>
		<TableColumn ref="dynQualifiedNameWidth2" Visible="false" Object.Use="assoc.dst" />
		<TableColumn ref="dynType" Sort="1" Object.Use="assoc.dst"/>
		<TableColumn ref="dynDef"  Sort="2" Object.Use="assoc.dst"/>
	</Table>


	<Table ID="BI_Contexts">
		<TableColumn ref="dynBpaName" Sort="0" />
		<TableColumn ref="dynQualifiedNameWidth2" Visible="false" Object.Use="assoc.dst" />
		<TableColumn ref="dynDefWidth2" />
		<TableColumn ref="dynBPContextPath" />
		<TableColumn ref="dynTags" Visible="false" />
	</Table>

	<Table ID="BP-HasContext-Name">
		<TableColumn ref="dynBpaNameNoHeader" Sort="0" />
	</Table>

	<Table ID="BP_MyBusinessItemList">
		<TableColumn ref="dynBpaMLNameWithIcon" Sort="0" MultiplexML="true" />
		<TableColumn ref="dynQualifiedNameWidth2" Visible="false" />
		<TableColumn ref="dynType" Sort="1" />
		<TableColumn ref="dynBpaMLDefinition"  MultiplexML="true" />
		<TableColumn ref="dynBPOwnedBy" Filterable ="false"/>
		<TableColumn ref="dynQualContextName" Visible="false" Filterable ="false"/>
		<TableColumn ref="dynBpaCrDate" Filterable ="false"/>
		<TableColumn ref="dynBpaModifDate" Filterable ="false"/>
		<TableColumn ref="dynBpaStatus" Filterable="false"/>
		<TableColumn ref="dynDesignatedAttsML" Visible="false" Filterable ="true" MultiplexML="true"/>
		<TableColumn ref="dynMgOwner" Visible="false" Filterable="false" />
		<Property Name="baseType" Value="BPK__BUSINESS_ITEM" />
		<Property Name="ignoreAttributesOfTypes" Value="RBG/BUSINESS-TERM, BPK_BUSINESS_RULE, BPK_PROCESS, BPK_CRITICAL_DATA_ELEMENT, GDP_PERSONAL_INFORMATION, GDP_PI_STRUCTURE, BPK_POLICY, BPK_STANDARD, BPK__CUST_TIMED_BUSINESS_ITEM, BPK__CUST_BUSINESS_ITEM" />
		<Property Name="pageSize" Value="*" />
		<Property Name="lineHeight" Value="1" />
	</Table>

	<TableColumn Title="%dynActionsDocBP" ID="dynActionsDocBP" Width="1" isAction="true">
		<ColumnProperty Renderer="com.rochade.wa.ui.editor.link.RemoveDocumentLinkActionCellRenderer" />
	</TableColumn>

	<TableColumn Title="%dynReferenceDocumentKindBP" ID="dynReferenceDocumentKindBP"   Width="3">
		<ColumnText Key="RBG/URL" />
		<ColumnText Key="RBG/DOCUMENT-TITLE" />
		<ColumnProperty Renderer="com.rochade.mg.cellrenderer.ReferenceDocumentKindCellRenderer" />
	</TableColumn>

	<Table ID="BP_REFDOCUMENTVIEW">
		<TableColumn ref="dynActionsDocBP" Filterable="false"/>
		<!-- TableColumn ref="dynBpaMLNameWithIcon" Visible="false" Sort="0" Object.Use="assoc.dst" /-->
		<TableColumn ref="dynReferenceDocumentKindBP" Filterable="false" Object.Use="assoc.dst" />
		<TableColumn ref="dynDefinition" Object.Use="assoc.dst" />
		<Property Name="pageSize" Value="5" />
		<Property Name="lineHeight" Value="1" />
	</Table>

	<TableColumn Title="%dynUser" ID="dynBpaHistUser">
		<ColumnList Direction="forward" ObjectType="RBG/USER" AssocType="RBG/MODIFIER">
			<ColumnText Key="name" />
