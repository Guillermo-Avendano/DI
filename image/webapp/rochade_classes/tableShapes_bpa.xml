	xsi:noNamespaceSchemaLocation="ShapeRes.xsd">

	<TableColumn Title="%dynBusinessContext" ID="dynBpaBusinessContext">
		<ColumnText Key="name" />
		<ColumnProperty Renderer="com.rochade.bpa.cellrenderer.BPContextCellRenderer" />
	</TableColumn>

	<TableColumn Title="%dynBPContextPath" ID="dynBPContextPath">
		<ColumnText Key="AbsName" />
