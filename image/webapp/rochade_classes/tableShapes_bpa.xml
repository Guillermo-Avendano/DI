		<ColumnProperty Renderer="com.rochade.mg.cellrenderer.MappedAttributeCellRenderer" />
	</TableColumn>

	<TableColumn Title="%dynHistRefByTask" ID="dynBpaHistRefByTask">
		<ColumnList Direction="backward" ObjectType="RWF/NOTIFICATION" AssocType="RWF/ITEM-HISTORY">
			<ColumnText Key="name" />
		</ColumnList>
		<ColumnProperty Renderer="com.rochade.mg.cellrenderer.HistTaskCellRenderer" />
	</TableColumn>

