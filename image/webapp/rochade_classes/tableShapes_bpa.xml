		<TableColumn ref="dynBpaOptionality" />
		<TableColumn ref="dynBpaLineage" />
		<Property Name="sortDirection" Value="descending" />
	</Table>
	
	<TableColumn Title="%dynLockedBy" ID="dynBpaLockedBy" Width="2">
		<ColumnText Key="RBG/LOCKED-BY" />
