<?xml-stylesheet type="text/xsl" href="Resources.xsl"?>
<ShapeResources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ShapeRes.xsd">

	<!-- general columns -->
	<TableColumn Title="%stiTemplName" ID="stiTemplName" Width="1">
		<!-- Image Key="icon" / -->
		<ColumnText Key="name" />
		<ColumnProperty Renderer="com.rochade.sti.ui.view.renderer.templates.TemplateNameCellRenderer" />
	</TableColumn>
	<TableColumn Title="%stiTemplDescription" ID="stiTemplDescription" Width="2">
		<!-- Image Key="icon" / -->
		<ColumnText Key="DEFINITION" />
		<ColumnProperty Renderer="com.rochade.sti.ui.view.renderer.templates.TemplateDescriptionCellRenderer" />
	</TableColumn>
	
	<Table ID="STI_StitchingTemplatesTable">
		<TableColumn ref="stiTemplName" />
		<TableColumn ref="stiTemplDescription" />
		<Property Name="lineHeight" Value="1" />
		<Property Name="IShapeFiller" Value="com.rochade.vis.srap.model.ColumnShapeFiller" />
	</Table>
	
	<Table ID="STI_StitchingContainersTable">
		<TableColumn ref="dynRbrName" Sort="0" />
		<TableColumn ref="dynDefWidth3" />
	</Table>
	
	<Table ID="STI_StitchingFromTo">
		<TableColumn ref="dynRbrName" Sort="0" Object.Use="assoc.dst"/>
		<TableColumn ref="dynType" Sort="1" Object.Use="assoc.dst"/>
		<TableColumn ref="dynQualifiedNameWidth2" Object.Use="assoc.dst"/>
		<TableColumn ref="dynDef" Object.Use="assoc.dst"/>
	</Table>
	<Table ID="STI_StitchingElements">
		<TableColumn ref="dynRbrName" Sort="0" Object.Use="assoc.dst"/>
		<TableColumn ref="dynType" Sort="1" Object.Use="assoc.dst"/>
		<TableColumn ref="dynDef" Object.Use="assoc.dst"/>
	</Table>
</ShapeResources>
